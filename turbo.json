{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"type-check": {
			"dependsOn": ["^type-check"],
			"cache": false
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		}
	}
}
